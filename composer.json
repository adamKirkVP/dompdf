{
    "name": "AdamKirkVP/dompdf",
    "type": "library",
    "description": "DOMPDF is a CSS 2.1 compliant HTML to PDF converter",
    "homepage": "https://github.com/adamKirkVP/dompdf",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Adam Kirk",
            "email": "adam.kirk@victoriaplum.com"
        }
    ],
    "autoload": {
        "psr-4" : {
            "Dompdf\\" : "src/"
        },
        "classmap" : ["lib/"]
    },
    "require": {
        "php": ">=5.3.0",
        "ext-gd": "*",
        "ext-dom": "*",
        "ext-mbstring": "*",
        "phenx/php-font-lib": "0.4.*",
        "phenx/php-svg-lib": "0.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    }
}
